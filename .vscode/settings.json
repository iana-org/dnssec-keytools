{
        "ruff.fixAll": true,
        "editor.codeActionsOnSave": {
                "source.organizeImports": "explicit"
        },
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "python.envFile": "${workspaceFolder}/.vscode/.env",
        "isort.check": true,
        "isort.args": [
                "[\"--profile\",\"black\"]"
        ],
        "cSpell.words": [
                "algs",
                "CFGFILE",
                "dndepth",
                "DNSKEY",
                "dnspython",
                "DNSTTL",
                "domainname",
                "ECDSAP",
                "filelog",
                "GOST",
                "hexlify",
                "keydel",
                "keydelete",
                "keydigest",
                "keygen",
                "Keymaster",
                "Keyper",
                "keytag",
                "kirei",
                "KSKM",
                "ksrsigner",
                "libsofthsm",
                "NSEC",
                "peercert",
                "privkey",
                "pubkey",
                "pydantic",
                "Pylance",
                "rdata",
                "rdataset",
                "rrset",
                "RRSIG",
                "RSAMD",
                "RSASHA",
                "secp",
                "softhsm",
                "trustanchor",
                "unhexlify",
                "venv",
                "werkzeug",
                "WKSR"
        ]
}
